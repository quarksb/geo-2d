{
  "name": "geo-2d",
  "version": "0.0.0-beta.12",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/geometry.umd.js",
  "module": "dist/geometry.es.js",
  "exports": {
    ".": {
      "import": "./dist/geometry.es.js",
      "require": "./dist/geometry.umd.js"
    },
    "./package.json": "./package.json",
    "./README.md": "./README.md"
  },
  "types": "dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/src/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:demo",
    "build:demo": "vue-tsc && vite build --mode demo",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "publish": "vite build --mode lib && npm publish",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "less": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "peerDependencies": {
    "gl-matrix": "^3.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quarksb/organic-svg"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}